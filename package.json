{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "db:setup": "tsx src/db.ts",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:clean": "tsx src/db/clean.ts",
    "db:show": "tsx src/db/showTable.ts",
    "dev": "tsx --env-file .env src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist node_modules data package-lock.json",
    "postinstall": "cd node_modules/better-sqlite3 && pnpm run build-release"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "discord.js": "^14.22.1",
    "dotenv": "^16.4.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "node-gyp": "^10.1.0",
    "prettier": "^3.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
